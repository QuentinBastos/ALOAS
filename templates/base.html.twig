<!DOCTYPE html>
<html lang="fr">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8">

    <title>{% block title %}ALOAS{% endblock %}</title>
    <meta name="description" content="{% block meta_description %}ALOAS Gestion de tournoi{% endblock %}">
    <meta name="keywords" content="{% block meta_keywords %}ALOAS SAE TOURNOI LYON1 VALETTE IUT {% endblock %}">
    <meta name="author" content="{% block meta_author %}ALOAS{% endblock %}">
    <meta name="robots" content="{% block meta_robots %}index, follow{% endblock %}">
    <meta name="generator" content="ALOAS">
    <link rel="canonical" href="{% block canonical %}{{ app.request.uri }}{% endblock %}">
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('build/images/aloas-logo.png') }}">
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app_css') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
    <script src="https://kit.fontawesome.com/85b48dec81.js" crossorigin="anonymous"></script>
</head>
<body class="w-full">
{% block body %}{% endblock %}
</body>
</html>
